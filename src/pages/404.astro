---
import BaseLayout from "@layouts/BaseLayout.astro";

export const prerender = true;
---

<BaseLayout title="Page Not Found" description="This is a blog">
  <main class="bs-container-md mt-32 grow scroll-mt-16">
    <section
      class="prose prose-invert max-w-none grow text-center lg:prose-xl prose-headings:scroll-mt-16"
    >
      <h1>404: Not Found</h1>
      <p>Couldn't find what you were looking for :(</p>
    </section>
  </main>
</BaseLayout>
